{
    "name": "blackdante101/getinitials",
    "description": "returns name initials from string",
    "keywords": [
        "blackdante101",
        "getinitials"
    ],
    "homepage": "https://github.com/blackdante101/getinitials",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Abukari Einus",
            "email": "charismacaptain694@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Blackdante101\\Getinitials\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blackdante101\\Getinitials\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
