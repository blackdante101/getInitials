{
    "name": "getinitials/getinitials",
    "description": "returns name initials from string",
    "keywords": [
        "getinitials",
        "getinitials"
    ],
    "homepage": "https://github.com/blackdante101/getinitials",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Abukari Einus",
            "email": "charismacaptain694@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Getinitials\\Getinitials\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Getinitials\\Getinitials\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Getinitials\\Getinitials\\GetinitialsServiceProvider"
            ],
            "aliases": {
                "Getinitials": "Getinitials\\Getinitials\\GetinitialsFacade"
            }
        }
    }
}
